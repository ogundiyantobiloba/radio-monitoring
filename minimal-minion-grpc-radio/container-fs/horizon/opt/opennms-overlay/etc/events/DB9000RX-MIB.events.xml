<events xmlns="http://xmlns.opennms.org/xsd/eventconf">
    <event>
        <mask>
            <maskelement>
                <mename>id</mename>
                <mevalue>.1.3.6.1.4.1.35833.7.4</mevalue>
            </maskelement>
            <maskelement>
                <mename>generic</mename>
                <mevalue>6</mevalue>
            </maskelement>
            <maskelement>
                <mename>specific</mename>
                <mevalue>1</mevalue>
            </maskelement>
            <varbind>
                <vbnumber>2</vbnumber>
                <vbvalue>1</vbvalue>
            </varbind>
        </mask>

        <uei>uei.opennms.org/traps/DB9000RX-MIB/alarmTest</uei>
        <event-label>DB9000RX-MIB defined trap event: alarmTest</event-label>
        <descr>
            &lt;p> &lt;/p>&lt;table>
            &lt;tr>&lt;td>&lt;b>

            snmpAgentID&lt;/b>&lt;/td>&lt;td>
            %parm[#1]%;&lt;/td>&lt;td>&lt;p>&lt;/p>&lt;/td>&lt;/tr>
            &lt;tr>&lt;td>&lt;b>

            serial&lt;/b>&lt;/td>&lt;td>
            %parm[#2]%;&lt;/td>&lt;td>&lt;p>&lt;/p>&lt;/td>&lt;/tr>&lt;/table>
        </descr>
        <logmsg dest="logndisplay">&lt;p>
            alarmTest trap received
            snmpAgentID=%parm[#1]%
            serial=%parm[#2]%&lt;/p>
        </logmsg>
        <severity>Warning</severity>
        <alarm-data reduction-key="%uei%:%dpname%:%nodeid%" alarm-type="1" auto-clean="false"/>

    </event>
<!--    clear trap for the above-->
    <event>
        <mask>
            <maskelement>
                <mename>id</mename>
                <mevalue>.1.3.6.1.4.1.35833.7.4</mevalue>
            </maskelement>
            <maskelement>
                <mename>generic</mename>
                <mevalue>6</mevalue>
            </maskelement>
            <maskelement>
                <mename>specific</mename>
                <mevalue>1</mevalue>
            </maskelement>
            <varbind>
                <vbnumber>2</vbnumber>
                <vbvalue>0</vbvalue>
            </varbind>
        </mask>

        <uei>uei.opennms.org/traps/DB9000RX-MIB/alarmTest</uei>
        <event-label>DB9000RX-MIB defined trap event: alarmTest</event-label>
        <descr>
            &lt;p> &lt;/p>&lt;table>
            &lt;tr>&lt;td>&lt;b>

            snmpAgentID&lt;/b>&lt;/td>&lt;td>
            %parm[#1]%;&lt;/td>&lt;td>&lt;p>&lt;/p>&lt;/td>&lt;/tr>
            &lt;tr>&lt;td>&lt;b>

            serial&lt;/b>&lt;/td>&lt;td>
            %parm[#2]%;&lt;/td>&lt;td>&lt;p>&lt;/p>&lt;/td>&lt;/tr>&lt;/table>
        </descr>
        <logmsg dest="logndisplay">&lt;p>
            alarmTest trap received
            snmpAgentID=%parm[#1]%
            serial=%parm[#2]%&lt;/p>
        </logmsg>
        <severity>Normal</severity>
        <alarm-data reduction-key="%uei%:%dpname%:%nodeid%"
                    alarm-type="2"
                    clear-key="uei.opennms.org/traps/DB9000RX-MIB/alarmTest:%dpname%:%nodeid%"
                    auto-clean="false"/>


    </event>






<!--    second event -->
    <event>
        <mask>
            <maskelement>
                <mename>id</mename>
                <mevalue>.1.3.6.1.4.1.35833.7.4</mevalue>
            </maskelement>
            <maskelement>
                <mename>generic</mename>
                <mevalue>6</mevalue>
            </maskelement>
            <maskelement>
                <mename>specific</mename>
                <mevalue>2</mevalue>
            </maskelement>
        </mask>
        <uei>uei.opennms.org/traps/DB9000RX-MIB/alarmInputSwitch</uei>
        <event-label>DB9000RX-MIB defined trap event: alarmInputSwitch</event-label>
        <descr>
            &lt;p> &lt;/p>&lt;table>
            &lt;tr>&lt;td>&lt;b>

            snmpAgentID&lt;/b>&lt;/td>&lt;td>
            %parm[#1]%;&lt;/td>&lt;td>&lt;p>&lt;/p>&lt;/td>&lt;/tr>
            &lt;tr>&lt;td>&lt;b>

            currentSource&lt;/b>&lt;/td>&lt;td>
            %parm[#2]%;&lt;/td>&lt;td>&lt;p>
            url1(0)
            url2(1)
            url3(2)
            mp3player(3)
            icecastClient(4)
            rtpClient(5)
            &lt;/p>&lt;/td>&lt;/tr>&lt;/table>
        </descr>
        <logmsg dest="logndisplay">&lt;p>
            alarmInputSwitch trap received
            snmpAgentID=%parm[#1]%
            currentSource=%parm[#2]%&lt;/p>
        </logmsg>
        <severity>Warning</severity>
        <varbindsdecode>
            <parmid>parm[#2]</parmid>
            <decode varbindvalue="0" varbinddecodedstring="url1"/>
            <decode varbindvalue="1" varbinddecodedstring="url2"/>
            <decode varbindvalue="2" varbinddecodedstring="url3"/>
            <decode varbindvalue="3" varbinddecodedstring="mp3player"/>
            <decode varbindvalue="4" varbinddecodedstring="icecastClient"/>
            <decode varbindvalue="5" varbinddecodedstring="rtpClient"/>
        </varbindsdecode>
    </event>
</events>