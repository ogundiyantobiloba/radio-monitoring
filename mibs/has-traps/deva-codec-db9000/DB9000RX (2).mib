  MAX-ACCESS not-accessible
  STATUS current
  DESCRIPTION
    " "
  INDEX { streamNumber }
  ::= { streamTable 1 }
StreamEntry ::= SEQUENCE {
  streamNumber Integer32,
  url DisplayString,
  decoder INTEGER,
  sampleRate Integer32,
  channelsNumber Integer32,
  preBuffer Integer32,
  bufferingTout Integer32,
  gain Integer32
}
	   
